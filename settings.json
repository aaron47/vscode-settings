{
  "editor.semanticHighlighting.enabled": true,
  // "editor.fontSize": 15.5,
  // "editor.fontSize": 14.5,
  "editor.fontSize": 15.5,
  "window.zoomLevel": 0,
  // "editor.fontSize": 15.5,
  // "editor.cursorWidth": 25,
  // "editor.stickyScroll.enabled": true,
  // "editor.fontSize": 15,
  // "editor.lineHeight": 25,
  // "editor.fontFamily": "'JetBrains Mono','JetBrainsMonoNL NFM', 'Menlo', MesloLGS NF, 'Consolas'",
  "editor.fontFamily": "'Menlo NF', 'monospace', monospace",
  "editor.tabSize": 2,
  "editor.formatOnSave": false,
  "editor.formatOnSaveMode": "file",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "html.format.preserveNewLines": false,
  "html.format.wrapLineLength": 0,
  "explorer.confirmDelete": false,
  "workbench.editor.tabSizing": "shrink",
  "git.autofetch": true,
  "emmet.includeLanguages": {
    "typescriptreact": "typescriptreact",
    "javascript": "javascriptreact",
    "php": "html"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.singleQuote": true,
  "editor.inlineSuggest.enabled": true,
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDragAndDrop": false,
  "php.validate.executablePath": "/usr/bin/php",
  "tailwindCSS.includeLanguages": {
    "plaintext": "php, html"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-h>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<C-b>"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["g", "f"],
      "commands": ["editor.action.formatDocument"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["_"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    }
  ],
  "material-icon-theme.activeIconPack": "angular",
  "workbench.preferredDarkColorTheme": "Winter is Coming (Dark Blue - No Italics)",
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "zenMode.hideTabs": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  // "editor.letterSpacing": 0.2,
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "security.workspace.trust.untrustedFiles": "open",
  "window.titleBarStyle": "custom",
  "workbench.productIconTheme": "developer-icons",
  "workbench.list.smoothScrolling": true,
  // "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "solid",
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "workbench.statusBar.visible": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "search.actionsPosition": "auto",
  "window.commandCenter": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "Menlo NF",
  "editor.tokenColorCustomizations": {   
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  }
}
